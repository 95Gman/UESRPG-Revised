d style=\"height: 16px; width: 84.9022%;\">Up to five sentences</td>\n</tr>\n<tr style=\"height: 16px;\">\n<td style=\"height: 16px; width: 14.0249%; text-align: center;\">5</td>\n<td style=\"height: 16px; width: 84.9022%;\">As many words as the character desires</td>\n</tr>\n<tr style=\"height: 16px;\">\n<td style=\"height: 16px; width: 14.0249%; text-align: center;\">6</td>\n<td style=\"height: 16px; width: 84.9022%;\">Images</td>\n</tr>\n<tr style=\"height: 16px;\">\n<td style=\"height: 16px; width: 14.0249%; text-align: center;\">7</td>\n<td style=\"height: 16px; width: 84.9022%;\">Complex feelings and concepts</td>\n</tr>\n</tbody>\n</table>","halfSpeed":false,"addHalfSpeed":false,"halfFatiguePenalty":false,"mechanical":false,"crpCost":null,"halfWoundPenalty":false,"painIntolerant":false},"sort":800001,"flags":{"core":{"sourceId":"Item.wtEFksGPcojtDLTj"}},"img":"systems/uesrpg-d100/images/Icons/forceOfWill.webp","effects":[],"folder":null,"permission":{"default":0}},{"_id":"YGUCokJpdBCMYA8l","name":"Thought Caster","type":"talent","data":{"mpBonus":0,"hpBonus":0,"spBonus":0,"lpBonus":0,"wtBonus":0,"iniBonus":0,"speedBonus":0,"flyBonus":0,"swimBonus":0,"diseaseR":0,"fireR":0,"frostR":0,"shockR":0,"poisonR":0,"magicR":0,"natToughnessR":0,"silverR":0,"sunlightR":0,"replace":{"ini":{"iniToggle":false,"value":0,"characteristic":"str"},"wt":{"wtToggle":false,"value":0,"characteristic":"str"}},"characteristics":{"str":"STR","end":"END","agi":"AGI","int":"INT","wp":"WP","prc":"PRC","prs":"PRS","lck":"LCK"},"characteristicBonus":{"strChaBonus":0,"endChaBonus":0,"agiChaBonus":0,"intChaBonus":0,"wpChaBonus":0,"prcChaBonus":0,"prsChaBonus":0,"lckChaBonus":0},"description":"<p>The character does not need to speak or have hands free in order to cast spells.</p>","xpCost":800,"level":6,"talentReq":"Spell Sword","miscReq":"Willpower 50","halfFatiguePenalty":false,"halfWoundPenalty":false,"untrainedException":false,"fatigueException":false,"addIntToMP":false,"armorAcrobaticsExc